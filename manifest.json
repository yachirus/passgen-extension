{
  "manifest_version": 2,
  "name": "passgen-extension",
  "description": "Password generator extension",
  "version": "0.0.0.1",
  "icons": {"48": "passgen.png"},
  "background": {
    "scripts": ["background.js"]
    },
  "browser_action": {
    "default_icon": "passgen.png",
    "default_title": "Password generator"
  },
  "permissions": ["clipboardWrite"],
  "options_page": "options.html"
}